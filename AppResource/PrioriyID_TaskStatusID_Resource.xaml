<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=netstandard">
    <!-- Recurso para actualizar las prioridades estados de las tareas y sus ID segun la base de datos-->
    <system:String x:Key="HighPriority">High</system:String>
    <system:String x:Key="MediumPriority">https://localhost:44384/api/Tasks/</system:String>
    <system:String x:Key="LowPriority">https://localhost:44384/api/Tasks</system:String>
    <system:Int32 x:Key="IDLowPriority2">2</system:Int32>
    <system:Int32 x:Key="IDMediumPriority">1</system:Int32>
    
</ResourceDictionary>